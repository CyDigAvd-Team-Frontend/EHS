<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Popover
        title="{Tittel}"
        class="sapUiContentPadding"
        placement="Auto"
        contentMinWidth="300px"
    >
        <VBox>
            <Label text="{i18n>ColumnPerson}" />
            <Text
                text="{Name}"
                class="sapUiSmallMarginBottom"
            />
            <Label text="{i18n>ColumnRole}" />
            <VBox
                id="_IDGenVBox1"
                items="{Rolle}"
                class="sapUiSmallMarginBottom"
            >
                <Text
                    id="_IDGenText13"
                    text="{}"
                />
            </VBox>
            <Label text="{i18n>ColumnAno}" />
            <Text text="Nei" />
        </VBox>
    </Popover>
</core:FragmentDefinition>

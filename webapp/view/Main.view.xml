<mvc:View
    xmlns:core="sap.ui.core"
    controllerName="no.mil.zehs.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    height="100%"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:mc="sap.suite.ui.microchart"
>
    <Page
        id="page"
        title="Status på mine saker"
        showNavButton="true"
        navButtonPress="onPressNavToHome"
    >
        <content>
            <fb:FilterBar
                id="_IDGenFilterBar1"
                reset="onReset"
                search="onSearch"
                showRestoreButton="true"
                showClearButton="true"
            >
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        id="_IDGenFilterGroupItem1"
                        groupName="__$INTERNAL$"
                        name="A"
                        label="Dato"
                        partOfCurrentVariant="true"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <DatePicker id="_IDGenDatePicker1" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        id="_IDGenFilterGroupItem2"
                        groupName="__$INTERNAL$"
                        name="B"
                        label="Status"
                        partOfCurrentVariant="true"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <Select id="_IDGenSelect1">
                                <items>
                                    <core:Item
                                        id="_IDGenItem1"
                                        text="{i18n>SelectAll}"
                                    />
                                    <core:Item
                                        id="_IDGenItem2"
                                        text="{i18n>SelectDone}"
                                    />
                                    <core:Item
                                        id="_IDGenItem3"
                                        text="{i18n>SelectWorking}"
                                    />
                                    <core:Item
                                        id="_IDGenItem4"
                                        text="{i18n>SelectReceived}"
                                    />
                                    <core:Item
                                        id="_IDGenItem5"
                                        text="{i18n>SelectReopen}"
                                    />
                                </items>
                            </Select>
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        id="_IDGenFilterGroupItem3"
                        groupName="__$INTERNAL$"
                        name="C"
                        label="Type"
                        partOfCurrentVariant="true"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <Select id="_IDGenSelect2">
                                <items>
                                    <core:Item
                                        id="_IDGenItem6"
                                        text="Hendelse"
                                    />
                                    <core:Item
                                        id="_IDGenItem7"
                                        text="Varsel"
                                    />
                                </items>
                            </Select>
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="D"
                        label="Roller"
                        partOfCurrentVariant="true"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <MultiComboBox>
                                <core:Item text="Registrerende person" />
                                <core:Item text="Registrert på vegne av" />
                                <core:Item text="Skadet person" />
                                <core:Item text="Varsler" />
                                <core:Item text="Vitne" />
                                <core:Item text="Saksbehandler" />
                                <core:Item text="MV" />
                            </MultiComboBox>
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
            <Toolbar
                id="_IDGenToolbar1"
                class="sapUiTinyMarginEnd"
            >
                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                <!-- <Button
                    id="_IDGenButton2"
                    icon="sap-icon://add"
                    text="{i18n>ButtonToNotice}"
                    press="onPressNavToVarsel"
                />
                <ToolbarSeparator id="_IDGenToolbarSeparator1" /> -->
                <Button
                    id="_IDGenButton1"
                    icon="sap-icon://add"
                    text="{i18n>ButtonToCase}"
                    press="onPressNavToForm"
                />
            </Toolbar>
            <VBox renderType="Bare">
                <Table
                    width="100%"
                    contextualWidth="Auto"
                    id="_IDGenTable1"
                    items="{ListModel>/}"
                    fixedLayout="false"
                >
                    <columns>
                        <Column
                            id="_IDGenColumn5"
                            minScreenWidth="Tablet"
                            demandPopin="true"
                        >
                            <Text
                                id="_IDGenText8"
                                text="{i18n>ColumnId}"
                            />
                        </Column>

                        <Column
                            id="_IDGenColumn1"
                            minScreenWidth="Tablet"
                            demandPopin="true"
                        >
                            <Text
                                id="_IDGenText1"
                                text="{i18n>ColumnType}"
                            />
                        </Column>
                        <Column
                            id="_IDGenColumn6"
                            minScreenWidth="Tablet"
                            demandPopin="true"
                        >
                            <Text
                                id="_IDGenText9"
                                text="{i18n>ColumnDate}"
                            />
                        </Column>
                        <Column id="_IDGenColumn7">
                            <Text
                                id="_IDGenText10"
                                text="{i18n>ColumnTitle}"
                            />
                        </Column>
                        <!-- <Column
                            id="_IDGenColumn8"
                            minScreenWidth="Tablet"
                        >
                            <Text
                                id="_IDGenText11"
                                text="{i18n>ColumnRole}"
                            />
                        </Column>
                        <Column
                            id="_IDGenColumn2"
                            minScreenWidth="Tablet"
                        >
                            <Text
                                id="_IDGenText2"
                                text="{i18n>ColumnPerson}"
                            />
                        </Column>-->
                        <Column id="_IDGenColumn3">
                            <Text
                                id="_IDGenText3"
                                text="{i18n>ColumnStatus}"
                            />
                        </Column>

                        <Column
                            id="_IDGenColumn4"
                            width="5em"
                        />
                    </columns>
                    <items>
                        <ColumnListItem
                            id="_IDGenColumnListItem1"
                            highlight="{path: 'ListModel>Status', formatter: '.formatter.status'}"
                        >
                            <cells>
                                <Text
                                    id="_IDGenText7"
                                    text="{ListModel>ID}"
                                />

                                <Text
                                    id="_IDGenText5"
                                    text="{ListModel>Type}"
                                />
                                <Text
                                    id="_IDGenText6"
                                    text="{ListModel>Dato}"
                                />
                                <Text
                                    id="_IDGenText12"
                                    text="{ListModel>Tittel}"
                                />
                                <!-- <VBox
                                    id="_IDGenVBox1"
                                    items="{ListModel>Rolle}"
                                >
                                    <Text
                                        id="_IDGenText13"
                                        text="{ListModel>}"
                                    />
                                </VBox>

                                <Text
                                    id="_IDGenText14"
                                    text="{ListModel>Name}"
                                /> -->
                                <Text
                                    id="_IDGenText15"
                                    text="{ListModel>Status}"
                                />

                                <FlexBox
                                    id="_IDGenFlexBox1"
                                    justifyContent="SpaceBetween"
                                >
                                    <!-- <tnt:InfoLabel
                                    id="il1"
                                    text="1"
                                    visible="{= ${ListModel>Personer/Kommentar/0/Author} !== 'Lars'  &amp;&amp; ${ListModel>Kommentar}.length > 0 ? true :false }"
                                    renderMode="Narrow"
                                    colorScheme="2"
                                    displayOnly="true"
                                /> -->
                                    <!-- <Button
                                    id="_IDGenButton3"
                                    text="Melding til saksbehandler"
                                    press="onPressNavToFeed"
                                /> -->
                                    <Button
                                        type="Ghost"
                                        icon="sap-icon://hint"
                                        press="onPressOpenInfo"
                                        class="sapUiTinyMarginEnd"
                                        tooltip="Informasjon"
                                    />
                                    <Button
                                        icon="sap-icon://discussion"
                                        press="onPressNavToFeed"
                                        type="{= ${ListModel>ID} === 1234 ? 'Emphasized' : 'Ghost'}"
                                        tooltip="Melding til saksbehandler"
                                        text="{= ${ListModel>ID} === 1234 ? 'Ny' : ''}"
                                        iconFirst="false"
                                    />
                                </FlexBox>
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
            </VBox>
        </content>
    </Page>
</mvc:View>
